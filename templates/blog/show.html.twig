{% extends 'base.html.twig' %}

{% block title %}
	Mon article 12
{% endblock %}

{% block body %}

	<article>
		<h2>{{ article.title }}</h2>
		<div class="metadata">
			Ecrit le
			{{ article.createAt | date('d/m/Y') }}
			à
			{{ article.createAt | date('H:i') }}
			dans la catégorie Politique</div>
		<div class="content">
			<img src="{{ article.image }}" alt="img1">
			<p>
				{{ article.content | raw }}
			</p>

		</div>
	</article>
{% endblock %}
